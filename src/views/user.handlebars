<div>
    <h1>USUARIOS:</h1>
    <ul>
        {{#each docs}}
        <li>
            <p>first_name: {{first_name}}, Apellido: {{last_name}}, Email: {{email}}</p>
        </li>
        {{/each}}
    </ul>

    {{#if hasPrevPage}}
    <a href="/users?page={{prevPage}}"> <button>Anterior</button> </a>
    {{/if }}

    {{#if hasNextPage}}
    <a href="/users?page={{nextPage}}"> <button>Siguiente</button> </a>
    {{/if }}
</div>